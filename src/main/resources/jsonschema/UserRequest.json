{
    "schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "name": {
            "type": [
                "null",
                "string"
            ]
        },
        "login": {
            "type": "string"
        },
        "password": {
            "type": "string"
        },
        "creationDate": {
            "type": "string",
            "format": "date-time"
        }
    },
    "required": [
        "login",
        "password",
        "creationDate"
    ]
}